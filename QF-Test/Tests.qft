    <Package id="_2Yvm" name="Flat planning">
      <Procedure id="_2Yvn" name="Get order of documents">
        <variable name="document 0"/>
        <variable name="document 1"/>
        <FetchGeometryStep client="$(client)" component="$(document 0)"
                           id="_2Yvo" local="true" xvarname="x0"
                           yvarname="y0"/>
        <FetchGeometryStep client="$(client)" component="$(document 1)"
                           id="_2Yvp" local="true" xvarname="x1"
                           yvarname="y1"/>
        <IfSequence id="_2YwV" name="Documents in the same row"
                    test="abs($(y0) - $(y1)) &lt;= 10">
          <ReturnStep id="_2Yvr" retval="$[$(x1) - $(x0)]"/>
          <ElseSequence id="_2YwW" name="Documents in different rows">
            <ReturnStep id="_2YwX" retval="$[$(y1) - $(y0)]"/>
          </ElseSequence>
        </IfSequence>
      </Procedure>
      <Procedure id="_2YvM" name="Switch documents">
        <variable name="document move icon 0"/>
        <variable name="document move icon 1"/>
        <FetchGeometryStep client="$(client)" component="Main composite"
                           id="_2YvN" local="true" xvarname="dx"
                           yvarname="dy"/>
        <FetchGeometryStep client="$(client)"
                           component="$(document move icon 0)"
                           heightvarname="h0" id="_2YvO" local="true"
                           widthvarname="w0" xvarname="x0" yvarname="y0"/>
        <FetchGeometryStep client="$(client)"
                           component="$(document move icon 1)"
                           heightvarname="h1" id="_2YvP" local="true"
                           widthvarname="w1" xvarname="x1" yvarname="y1"/>
        <MouseEventStep clicks="1" client="$(client)"
                        component="Main composite" event="MOUSE_PRESSED"
                        hardevent="true" id="_2YvQ" modifiers="16"
                        x="$[$(x1) + $(w1)/2 - $(dx)]"
                        y="$[$(y1) + $(h1)/2 - $(dy)]"/>
