    </Package>
    <Package id="_2YuI" name="Content">
      <Procedure id="_2Yue" name="Click arrow">
        <variable name="item"/>
        <FetchGeometryStep client="$(client)" component="$(item)"
                           heightvarname="h" id="_2Yuf" local="true"
                           xvarname="x" yvarname="y"/>
        <FetchGeometryStep client="$(client)" component="React.body"
                           id="_2Yuk" local="true" xvarname="dx"
                           yvarname="dy"/>
        <MouseEventStep clicks="1" client="$(client)" component="React.body"
                        event="MOUSE_MPRC" hardevent="true" id="_2Yug"
                        modifiers="16" x="$[$(x) - 12 - $(dx)]"
                        y="$[$(y) + $(h)/2 - $(dy)]"/>
      </Procedure>
